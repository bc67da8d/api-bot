cd overrideHuyen
pwd

INPUT_FILE="../_huyen.json"
TARGET_FILE="../db.json"
LOG_FILE="../log.txt"

echo "Ok!!START OVERRIDE"
cp $INPUT_FILE ./_input.json
cp $TARGET_FILE ./_target.json
python ./override.py

if (json_verify -q < ./_output.json)
then
	echo "RELEASE OVERRIDE"
	cp ./_output.json $TARGET_FILE
	rm ./_input.json
	rm ./_target.json
	rm ./_output.json
else
	bash "$CONTEXT/redMessage" "$1 structure check failed"
	echo "NOT OVERRIDE"
	LOG="OVERRIDE $INPUT_FILE"
	echo $LOG >> $LOG_FILE
fi
